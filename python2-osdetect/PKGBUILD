pkgname=python2-osdetect
pkgver=???
pkgrel=1
pkgdesc="A Python 2 module to detect the operating system."
arch=(any)
url="https://github.com/malte70/OSDetect"
license=(BSD)
depends=(python2)
makedepends=("python2-distribute")
source=("https://github.com/downloads/malte70/python-twoffein/Twoffein-${pkgver}.tar.gz")

package() {
	cd "${srcdir}/Twoffein-${pkgver}"
	python2 setup.py install --root="${pkgdir}/" --optimize=1
}
md5sums=('84596b0a6b8abf89f8394f213898ab6d')
